// ðŸ§© 3. Move All Zeros to the End (without extra space)
// Example: Input: [0, 1, 0, 3, 12]
// Output: [1, 3, 12, 0, 0]

const moveZerosToEnd = (arr) => {
  let i = 0;
  
  for (let j = 0; j < arr.length; j++) {
    // console.log(i,j)
    if (arr[j] !== 0) {
      [arr[i], arr[j]] = [arr[j], arr[i]]; 
      i++;
    }
  }
  
  console.log(arr);
}

moveZerosToEnd([0, 1, 0,0,0,0,0,0, 3, 12]); 
