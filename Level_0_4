// ðŸ§© 5. Merge Two Sorted Arrays
// Input: [1, 3, 5], [2, 4, 6] â†’ Output: [1, 2, 3, 4, 5, 6] with two pointer

const printInOrder = (arr1, arr2) => {
  let i = 0, j = 0;
  const finArr = [];
  while (i < arr1.length || j < arr2.length) {
    if (arr1[i] < arr2[j]) {
      console.log(arr1[i]);
      finArr?.push(arr1[i]);
      i++;
    } else {
      console.log(arr2[j]);
      finArr?.push(arr2[j]);
      j++;
    }
  }
console.log(finArr)
};

printInOrder([1, 3, 5], [2, 4, 6]);
